{
  "presets": ["es2015", "@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    "emotion",
    "transform-class-properties",
    "transform-react-jsx",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"]
      }],
      ["import-rename", {"^(.*)\\.jsx$": "$1"}]
    ]
  }
